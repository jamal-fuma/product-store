page.replace_html 'pages_list', 
                  :partial => 'pages/list', 
                  :locals => {:pages => @pages, :reorder_url => @reorder_url }
page.sortable('all_pages', :url => @reorder_url, :complete => "update_all();")
page << "update_all();"
page.reload if @pages.empty?
page["all_pages"].visual_effect :highlight
